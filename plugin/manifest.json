{
    "name" : "goto-music",
    "description" : "Skip YouTube music video intros",
    "manifest_version": 2,
    "version" : "1.0",
    
    "content_scripts" : [{
        "matches" : ["https://*.youtube.com/watch?v=*"],
        "js" : ["js-dependencies/firebase-app.js", 
                "js-dependencies/firebase-database.js",
                "js-dependencies/firebase-auth.js",
                "content.js"]
    }]
}
